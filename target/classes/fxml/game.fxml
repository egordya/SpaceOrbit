<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<fx:root fx:id="anchor" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" type="AnchorPane" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <FlowPane fx:id="lvlSelectorPane" alignment="CENTER" hgap="10" prefHeight="300" prefWidth="400">
      </FlowPane>
      <HBox fx:id="winBox">
         <Button layoutX="36.0" layoutY="14.0" mnemonicParsing="false" onAction="#restart" text="Restart" />
         <Button layoutX="36.0" layoutY="14.0" mnemonicParsing="false" onAction="#nextLevel" text="Next Level" />
         <Button layoutX="36.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToMainMenu" text="main menu test" />
      </HBox>
      <Pane fx:id="renderSurface" layoutX="10.0" layoutY="86.0" prefHeight="277.0" prefWidth="580.0" />
      <Button layoutX="36.0" layoutY="14.0" mnemonicParsing="false" onAction="#pause" text="Toggle Pause" />
      <Button layoutX="131.0" layoutY="14.0" mnemonicParsing="false" onAction="#resume" text="Start" />
      <Button layoutX="185.0" layoutY="14.0" mnemonicParsing="false" onAction="#restart" text="Restart" />
      <Button layoutX="491.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToMainMenu" text="main menu" />
   </children>
   <stylesheets>
      <URL value="@game.css" />
   </stylesheets>
</fx:root>
