<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>


<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<StackPane fx:id="root" xmlns:fx="http://javafx.com/fxml" prefHeight="200" prefWidth="320" style="-fx-background-color: Black;" >
    <children>
        <VBox fx:id="mainVbox" spacing="20.0" alignment="CENTER">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
            <Button fx:id="startContinue" onAction="#startGame" />
            <Button fx:id="lvlSelect"/>
            <Button text = "Customization" fx:id="customization" onAction="#customizationClicked"/>
            <Button text = "Level Builder" fx:id="lvlBuild"/>
            <Button text = "Settings" fx:id="settings"/>
            <Button fx:id="quit" onAction="#quit"/>
        </VBox>
        <VBox fx:id="customVbox" spacing="20.0" alignment="CENTER">
            <Button fx:id = "skins" text="skins"/>
            <Button fx:id="back" text="back" onAction="#backToMain"/>
        </VBox>
    </children>
</StackPane>